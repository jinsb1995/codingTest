package day_20210917;

import java.util.Arrays;
import java.util.List;

public class Maraton {
	
	
	public static void main(String[] args) {
		String[] participant = {"leo", "kiki", "eden"};
		String[] completion = {"eden", "kiki"};
//		String[] participant = {"marina", "josipa", "nikola", "vinko", "filipa"};
//		String[] completion = {"josipa", "filipa", "marina", "nikola"};
//		String[] participant = {"mislav", "stanko", "mislav", "ana"};
//		String[] completion = {"stanko", "ana", "mislav"};
		
		solution(participant, completion);
		
	}
	
	static String solution(String[] participant, String[] completion) {
		String result = "";
		
		List<String> part = Arrays.asList(participant);
		List<String> comp = Arrays.asList(completion);
		
		
//		for(int i = 0; i < participant.length; i++) {
			for(int j = 0; j < comp.size(); j++) {
				if(part.indexOf(comp.get(j)) != -1) {
					part.remove(part.indexOf(comp.get(j)));
					System.out.println(part.indexOf(comp.get(j)));
				}
				
				
			}
//		}
		
		for(int i = 0; i < part.size(); i++) {
			System.out.println(part.get(i));
		}
			
//		System.out.println(Arrays.toString(participant));
//		System.out.println(Arrays.toString(completion));
		
		
		return result;
	}
}
